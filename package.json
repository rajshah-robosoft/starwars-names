{
  "name": "raj-shah-starwars-names",
  "version": "1.4.0",
  "description": "Get a random starwars name",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "check-coverage": "nyc report --reporter=lcov --check-coverage --lines 90 --branches 50 --statements 90 --functions 90",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test:single": "nyc mocha src/index.test.js && nyc report --reporter=lcov",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rajshah-robosoft/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Raj Shah <raj.shah@robosoftin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajshah-robosoft/starwars-names/issues"
  },
  "homepage": "https://github.com/rajshah-robosoft/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.5",
    "@semantic-release/release-notes-generator": "9.0.1",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "ghooks": "2.0.4",
    "mocha": "8.0.1",
    "nyc": "15.1.0",
    "semantic-release": "17.0.8"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
